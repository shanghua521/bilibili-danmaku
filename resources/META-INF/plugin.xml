<idea-plugin>
    <id>com.wang.bilibili.danmaku</id>
    <name>bilibili danmaku</name>
    <version>1.0</version>
    <vendor email="shanghua714@gmail.com" url="https://github.com/shanghua521/bilibili-danmaku">shanghua</vendor>

    <description><![CDATA[
        这是一个 bilibili 弹幕插件，方便直播的时候看弹幕用，这是一个 bilibili 弹幕插件，方便直播的时候看弹幕用<br>
        ]]></description>

    <change-notes><![CDATA[
        <em>第一个版本</em>
        ]]>
  </change-notes>

    <idea-version since-build="193.0"/>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.wang.bilibili.danmaku.main.BiliBliToolWindowFactory" id="BiliBiliToolWindow"
                    canCloseContents="false" anchor="right"/>
    </extensions>

</idea-plugin>