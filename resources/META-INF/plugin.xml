<idea-plugin>
    <id>com.wang.bilibili.danmaku</id>
    <name>Bilibili Danmaku</name>
    <version>1.1</version>
    <vendor email="shanghua714@gmail.com" url="https://github.com/shanghua521/bilibili-danmaku">shanghua</vendor>

    <description><![CDATA[
        This is a bilibili barrage plug-in, which is convenient for watching barrage during live broadcast
        ]]></description>

    <change-notes><![CDATA[
        <em>第一个版本</em>
        <em>chang description</em>
        ]]>
  </change-notes>

    <idea-version since-build="193.0"/>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.wang.bilibili.danmaku.main.BiliBliToolWindowFactory" id="BiliBiliToolWindow"
                    canCloseContents="false" anchor="right"/>
    </extensions>

</idea-plugin>